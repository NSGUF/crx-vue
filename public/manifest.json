{
	"update_url": "https://clients2.google.com/service/update2/crx",
	"manifest_version": 2,
	"name": "crx-vue手脚架",
	"version": "1.2.6",
	"default_locale": "en",
	"description": "crx-vue手脚架",
	"icons": {
		"48": "favicon.ico"
	},
	"browser_action": {
		"default_icon": {
			"48": "favicon.ico"
		},
		"default_title": "crx-vue手脚架",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html",
		"persistent": true
	},
	"content_scripts": [
		{
			"all_frames": false,
			"run_at": "document_start",
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"js": [
				"content_script.js"
			]
		}
	],
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Ctrl+B",
				"windows": "Ctrl+B",
				"mac": "MacCtrl+B"
			}
		}
	},
	"permissions": [
		"bookmarks",
		"storage",
		"tabs",
		"downloads",
		"chrome://favicon/",
		"http://*/*",
		"https://*/*",
		"notifications",
		"contextMenus"
	]
}
